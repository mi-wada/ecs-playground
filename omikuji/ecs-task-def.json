{
  "containerDefinitions": [
    {
      "cpu": 0,
      "essential": true,
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost/health || exit 1"
        ],
        "interval": 30,
        "retries": 3,
        "timeout": 5
      },
      "image": "091512051921.dkr.ecr.ap-northeast-1.amazonaws.com/ecsp/omikuji@sha256:3ec427eebe18458aa910e53970a3ddd001716de0937f09124a3f7ba39cdc94a8",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "{{ tfstate `aws_cloudwatch_log_group.main['ecsp-omikuji'].name` }}",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "name": "omikuji",
      "portMappings": [
        {
          "appProtocol": "http",
          "containerPort": 80,
          "hostPort": 80,
          "name": "omikuji-80-tcp",
          "protocol": "tcp"
        }
      ],
      "versionConsistency": ""
    }
  ],
  "cpu": "1024",
  "executionRoleArn": "{{ tfstate `aws_iam_role.ecs_task_execution.arn` }}",
  "family": "ecsp-omikuji",
  "ipcMode": "",
  "memory": "2048",
  "networkMode": "awsvpc",
  "pidMode": "",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "runtimePlatform": {
    "cpuArchitecture": "ARM64",
    "operatingSystemFamily": "LINUX"
  }
}
