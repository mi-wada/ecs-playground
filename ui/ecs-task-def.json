{
  "containerDefinitions": [
    {
      "cpu": 0,
      "environment": [
        {
          "name": "OMIKUJI_BACKEND",
          "value": "http://omikuji/omikuji"
        }
      ],
      "essential": true,
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost/health || exit 1"
        ],
        "interval": 30,
        "retries": 3,
        "timeout": 5
      },
      "image": "091512051921.dkr.ecr.ap-northeast-1.amazonaws.com/ecsp/ui@sha256:7a2a28140a8326d2393debb45bdd35ea2be0463ecafc078da20d8457244bac72",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "{{ tfstate `aws_cloudwatch_log_group.main['ecsp-ui'].name` }}",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "name": "ui",
      "portMappings": [
        {
          "appProtocol": "http",
          "containerPort": 80,
          "hostPort": 80,
          "name": "ui-80-tcp",
          "protocol": "tcp"
        }
      ],
      "versionConsistency": ""
    }
  ],
  "cpu": "1024",
  "taskRoleArn": "{{ tfstate `aws_iam_role.ecs_task.arn` }}",
  "executionRoleArn": "{{ tfstate `aws_iam_role.ecs_task_execution.arn` }}",
  "family": "ecsp-ui",
  "ipcMode": "",
  "memory": "2048",
  "networkMode": "awsvpc",
  "pidMode": "",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "runtimePlatform": {
    "cpuArchitecture": "ARM64",
    "operatingSystemFamily": "LINUX"
  }
}
